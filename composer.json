{
  "name": "rebelinteractivegroup/rig-sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme.",
  "homepage": "https://roots.io/sage/",
  "authors": [
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    },
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "QWp6t",
      "email": "hi@qwp6t.me",
      "homepage": "https://github.com/qwp6t"
    },
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me",
      "homepage": "https://github.com/log1x"
    },
    {
      "name": "Rob Voss",
      "email": "rob.voss@digerati.io",
      "homepage": "https://github.com/Rob-Voss"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/roots/sage/issues",
    "forum": "https://discourse.roots.io/"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:rebelinteractivegroup/sage-bootstrap5-navwalker.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:rebelinteractivegroup/advanced-custom-fields-pro.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "roots/acorn": "^v3.3.1",
    "garrettweinberg/sage-bootstrap5-navwalker": "*",
    "log1x/acf-composer": "^2.1",
    "rebelinteractivegroup/advanced-custom-fields-pro": "*"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
    "laravel/pint": "^1.13"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  },
  "extra": {
    "acorn": {
      "providers": [
        "App\\Providers\\ThemeServiceProvider"
      ]
    }
  }
}
