{
  "name": "rebelinteractivegroup/rig-sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme.",
  "homepage": "https://roots.io/sage/",
  "authors": [
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    },
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "QWp6t",
      "email": "hi@qwp6t.me",
      "homepage": "https://github.com/qwp6t"
    },
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me",
      "homepage": "https://github.com/log1x"
    },
    {
      "name": "Rob Voss",
      "email": "rob.voss@digerati.io",
      "homepage": "https://github.com/Rob-Voss"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/roots/sage/issues",
    "forum": "https://discourse.roots.io/"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
    },
    {
      "type": "vcs",
      "url": "git@github.com:mwdelaney/google-tag-manager-snippets.git"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "garrettweinberg/sage-bootstrap5-navwalker": "*",
    "mwdelaney/sage-acf-gutenberg-blocks": "*",
    "mwdelaney/sage-acf-menu-icons": "*",
    "mwdelaney/sage-advanced-custom-fields": "*",
    "mwdelaney/sage-fonts-preload": "*",
    "mwdelaney/sage-wp-nav-submenu": "*",
    "mwdelaney/google-tag-manager-snippets": "*",
    "mwdelaney/add-sitemap-to-robots": "*",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "*",
    "wpackagist-plugin/resmushit-image-optimizer": "*",
    "wpackagist-plugin/regenerate-thumbnails": "*",
    "wpackagist-plugin/disable-comments": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/yoast-test-helper":"*",
    "wpackagist-plugin/redirection": "*",
    "wpackagist-plugin/safe-svg": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.7.1"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0).",
    "log1x/sage-svg": "A useful SVG directive for inlining SVG's within Blade views (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true,
      "ffraenz/private-composer-installer": true
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --extensions=php --standard=PSR12 app"
    ]
  },
  "extra": {
    "installer-paths": {
      "../../mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "../../plugins/{$name}/": ["type:wordpress-plugin"]
    },
    "acorn": {
      "providers": [
        "App\\Providers\\ThemeServiceProvider"
      ]
    }
  }
}
